namespace com.example

vartype Person {
  id: PersonId
  `record`: PersonRecord
}

integer PersonId

record PersonRecord {
  id: polymorphic PersonId
  firstName: String
  bestFriend: polymorphic Person default id//Record
  friends: list[PersonRecord]
  friendsMap: map[PersonId, Person default id]
  ids: list[PersonId]
  idss: list[list[PersonId]]
  persons: list[Person]
  personss: list[list[Person]]
  strings: map[String, String]
  mapppsss: map[map[list[String], list[Integer]], map[UserId, map[list[UserId], User default id]]]
}

list[Person/* default id*/] PersonList

vartype Bad {
  //bad: Person
}
