namespace com.example

vartype Person {
  id: PersonId
  `record`: PersonRecord
}

integer PersonId

record PersonRecord {
  id: polymorphic PersonId
  firstName: String
  bestFriend: polymorphic Person default id//Record
  friends: list[PersonRecord]
  ids: list[PersonId]
  idss: list[list[PersonId]]
  persons: list[Person]
  personss: list[list[Person]]
}

list[Person default id] PersonList
