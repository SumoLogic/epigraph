namespace com.example

vartype Person default id {
  id: PersonId
  `record`: PersonRecord
}

integer PersonId

record PersonRecord {
  id: polymorphic PersonId
  bestFriend: polymorphic Person//Record
  friends: list[PersonRecord]
  ids: list[PersonId]
  idss: list[list[PersonId]]
  persons: list[Person]
  personss: list[list[Person]]
}
