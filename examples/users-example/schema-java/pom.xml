      <artifactId>epigraph-java-core</artifactId>
      <version>0.0-SNAPSHOT</version>
    </dependency>

  </dependencies>

  <build>

    <plugins>

      <plugin>
        <groupId>com.sumologic.epigraph</groupId>
        <artifactId>epigraph-maven-plugin</artifactId>
      </plugin>

      <plugin>
        <groupId>com.sumologic.epigraph</groupId>
        <artifactId>epigraph-java-maven-plugin</artifactId>
      </plugin>

    </plugins>

  </build>

</project>
